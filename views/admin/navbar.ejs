<script>
function checkHref(){

    if(location.href.includes("addcustomer") || location.href.includes("allcustomer")|| location.href.includes("show-customer/")){
        document.getElementById("cust-col").classList.add("active");
        document.getElementById("cust-sub").classList.add("visible");
        
    }
    else if(location.href.includes("bill-list") || location.href.includes("sale-product")){
        document.getElementById("sale-col").classList.add("active");
        document.getElementById("sale-sub").classList.add("visible");
    }
    else if(location.href.includes("addpurchase") || location.href.includes("all-purchases") || location.href.includes("stocks")){
        document.getElementById("pur-col").classList.add("active");
        document.getElementById("pur-sub").classList.add("visible");
    }
    else if(location.href.includes("add-party") || location.href.includes("parties") || location.href.includes("show-party/") || location.href.includes("view-vendor-order/")){
        document.getElementById("par-col").classList.add("active");
        document.getElementById("par-sub").classList.add("visible");
    }
    else if(location.href.includes("total-exp") || location.href.includes("total-purchase")|| location.href.includes("edit_credit") || location.href.includes("credits")|| location.href.includes("credit_history")){
        document.getElementById("exp-col").classList.add("active");
        document.getElementById("exp-sub").classList.add("visible");
    }
    else if(location.href.includes("mdetails")){
        document.getElementById("medi-det").classList.add("active");
    }
    
}
checkHref();
</script>